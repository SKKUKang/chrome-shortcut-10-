{
    "manifest_version": 3,
    "name": "More Than 10 Shortcuts",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "version": "1.0",
    "description": "Unlock the potential to use more than 10 shortcuts in Chrome!",
    "permissions": ["tabs"],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    }
  }